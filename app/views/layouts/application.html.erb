<!DOCTYPE html>
<html>
<head>
  <title>FirstApp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body style="background: #EEEEEE;">
<%= render :partial => 'partials/header_menu' %>
<ul>
<% flash.each do |key, value| %>
  <li> <%= value %> </li>
<% end %>
</ul>
<div class="container">
  <%= yield %>
</div>

</body>
</html>
